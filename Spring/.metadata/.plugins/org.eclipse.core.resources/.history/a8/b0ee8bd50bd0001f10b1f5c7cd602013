package in.mbtions.program3_annotation.beans;

public class Student {
	private String name;
	private int rollno;
	private String email;
	private float marks;
	
	public void display() {
		System.out.println("Student object created!\n-------------------------------------");
		System.out.println("Name: "+name+"Roll no: "+rollno+"\nMarks: "+marks+"\nEmail: "+email);
	}
}
